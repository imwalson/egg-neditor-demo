{
  "name": "egg-neditor-demo",
  "version": "1.0.0",
  "description": "use neditor with egg.js",
  "keywords": [
    "node.js",
    "egg",
    "egg.js",
    "ueditor",
    "neditor",
    "rich editor"
  ],
  "dependencies": {
    "egg": "^1.8.0",
    "egg-multipart": "^2.0.0",
    "egg-oss": "^1.1.0",
    "egg-scripts": "^1.2.0",
    "egg-static": "^1.4.1",
    "egg-view-nunjucks": "^2.1.4",
    "lodash": "^4.17.5",
    "stream-wormhole": "^1.0.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "autod": "^2.9.0",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.12.0",
    "eslint": "^4.7.0",
    "eslint-config-egg": "^5.1.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "deploy": "egg-scripts start",
    "start": "egg-scripts start --daemon",
    "stop": "egg-scripts stop",
    "dev": "egg-bin dev --port 3001",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "6, 8"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:imwalson/egg-neditor-demo.git"
